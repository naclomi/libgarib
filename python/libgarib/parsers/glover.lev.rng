  <define name="CameoInst">
    <element name="CameoInst">
      <choice>
        <group>
          <!--Case 0x0-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">0|0x0*0</param>
            </data>
          </attribute>
          <ref name="CameoPlayAnimation"/>
        </group>
        <group>
          <!--Case 0x1-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">1|0x0*1</param>
            </data>
          </attribute>
          <ref name="CameoSetCameraAttention"/>
        </group>
        <group>
          <!--Case 0x2-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">2|0x0*2</param>
            </data>
          </attribute>
          <ref name="CameoInst2"/>
        </group>
        <group>
          <!--Case 0x3-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">3|0x0*3</param>
            </data>
          </attribute>
          <ref name="CameoSpin"/>
        </group>
        <group>
          <!--Case 0x4-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">4|0x0*4</param>
            </data>
          </attribute>
          <ref name="CameoGrabTodo"/>
        </group>
        <group>
          <!--Case 0x5-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">5|0x0*5</param>
            </data>
          </attribute>
          <ref name="CameoSetEnemyFlagTodo"/>
        </group>
        <group>
          <!--Case 0x6-->
          <attribute name="inst_type">
            <data type="string">
              <param name="pattern">6|0x0*6</param>
            </data>
          </attribute>
          <ref name="CameoLightningFlash"/>
        </group>
        <group>
          <!--Default case (only for types not listed above!)-->
          <attribute name="inst_type">
            <text/>
          </attribute>
          <ref name="CameoInstDefault"/>
        </group>
      </choice>
    </element>
  </define>
